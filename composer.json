{
  "name": "parsidev/telegram-bot-dialog",
  "description": "Telegram Bot API PHP SDK extension that allows to implement dialogs in bots",
  "type": "library",
  "homepage": "https://github.com/parsidev/telegram-bot-dialog",
  "license": "MIT",
  "version": "1.0.0",
  "authors": [
    {
      "name": "Mohammad Reza Parsaeian",
      "email": "programmer.net2009@live.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "BotDialogs\\": "src/"
    }
  },
  "require": {
    "php": "~7.0",
    "illuminate/support": "~5.5 | ~6.0 | ~7.0",
    "irazasyed/telegram-bot-sdk": "^3.0",
    "symfony/yaml": "~5.0",
    "predis/predis": "~1.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Telegram\\Bot\\Laravel\\TelegramServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev"
}
